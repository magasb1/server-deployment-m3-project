<!DOCTYPE html>
<html lang="en">
<%- include('partials/head', { title }) %>
<body class="" data-bs-theme="dark">
    <%- include('partials/header.ejs', { user })  %>
    <main class="container my-5">
        <h1>Pictures</h1>
        <% files.forEach((file) => { %>
            <div class="text-center">
              <img class="mb-3 img-fluid" src="/uploads/<%=file%>" style="max-width:250px">
              <p><%=file%></p>
            </div>
        <% });%>
    </main>
</body>
</html>